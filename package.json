{
    "name": "bio-syntax-colorer",
    "displayName": "Minimal Biosyntax Colorer",
    "description": "Custom overlay inspired by BioSyntax Entension, however made in current VSC and not version 1 so there aren't global issues",
    "publisher": "The A177",
    "version": "0.0.10",
    "repository": {
        "type": "git",
        "url": "https://github.com/MastorAaron/bio-syntax-colorer"
    },
    "main": "./extension.js",
    "activationEvents": [
        "onLanguage:fasta"
    ],
    "engines": {
        "vscode": "^1.85.0"
    },
    "contributes": {
        "languages": [
            {
                "id": "fasta",
                "aliases": ["FASTA"],
                "extensions": [
                    ".fastq",
                    ".fasta",
                    ".fa"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "fasta",
                "scopeName": "source.fasta",
                "path": "./syntaxes/fasta.tmLanguage.json"
            }
        ],
        "themes": [
            {
                "label": "FASTA BioSyntax",
                "uiTheme": "vs-dark",
                "path": "./fasta-colors.json"
            }
        ]
    }
}