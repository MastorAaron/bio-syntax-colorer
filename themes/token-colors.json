{
    "$schema": "vscode://schemas/color-theme",
    "type":"dark",
    "tokenColors":[
        {
            "name": "FASTA Header",
            "scope":"fasta.title",
            "settings":{
                "foreground":"#E6DB74",
                "fontStyle":"bold"
            }
        },
        {
            "name": "Adenine OR Alanine", 
            "scope": "source.fasta.ntA", 
            "settings": {
                "foreground": "#3ED132"
            }
        }, 
        {
            "name": "Cytosine OR Cysteine", 
            "scope": "source.fasta.ntC",
            "settings": {
                "foreground": "#4192FF"
            }
        }, 
        {
            "name": "Guanine OR Glycine", 
            "scope": "source.fasta.ntG", 
            "settings": {
                "foreground": "#F09000"
            }
        },
        {
            "name": "Thymine OR Threonine", 
            "scope": "source.fasta.ntT",
            "settings": {
                "foreground": "#FF0000"
            }
        }, 
        {
            "name": "Uracil OR Selenocysteine", 
            "scope": "source.fasta.ntU",
